<h1>Main#results</h1>
<p>Find me in app/views/main/results.html.erb</p>

<% @results.each do |item| %>
<div class="well">
<!-- 	<img src="<%= @photo %>" /> -->
	<h1><%= item["name"] %></h1>
	<p>Price Level: <%= item["price_level"] %></p>
	<p>Rating: <%= item["rating"] %></p>
	<p>Address: <%= item["vicinity"] %></p>
	<p>Photo ref: <%= @photo %></p>



</div>
<% end %>



